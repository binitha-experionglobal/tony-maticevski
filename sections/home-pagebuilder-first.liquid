{% endcomment %}

{% schema %}
{
  "name": "home-pagebuilder-first",
  "settings": [
    {
      "type": "checkbox",
      "id": "show_content",
      "label": "Show this section",
      "default": true
    }
  ],
  "blocks": [
    {
      "type": "image_block",
      "name": "Image Block",
      "settings": [
        {
          "type": "image_picker",
          "id": "imageurl",
          "label": "Choose an Image - Left section"
        }
      ]
    },
    {
      "type": "textarea_block",
      "name": "Text Content",
      "settings": [
        {
          "type": "textarea",
          "id": "text",
          "label": "Add text content"
        }
      ]
    },
    {
      "type": "text_block",
      "name": "Link Text",
      "settings": [
        {
          "type": "text",
          "id": "linkText",
          "label": "Add Explore Link Text"
        }
      ]
    },
    {
      "type": "video_block",
      "name": "Video Block",
      "settings": [
        {
          "type": "video",
          "id": "videourl",
          "label": "Choose a Video - Right section"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "home-pagebuilder-first",
      "blocks": [{ "type": "image_block" }, { "type": "textarea_block" }, { "type": "text_block" }]
    }
  ]
}
{% endschema %}
